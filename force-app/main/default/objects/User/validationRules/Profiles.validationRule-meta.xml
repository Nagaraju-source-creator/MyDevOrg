<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Profiles</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT($Profile.Name = &apos;Standard Platform User&apos; ),NOT($Profile.Name = &apos;System Administrator&apos; ))</errorConditionFormula>
    <errorMessage>Only for system admin and standard platform users</errorMessage>
</ValidationRule>
